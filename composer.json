{
    "name"        : "skyshabatura/taproot-2",
    "type"        : "wordpress-theme",
    "description" : "Taproot is a multipurpose theme built on Hybrid Core and the Mythic started theme",
    "keywords"    : ["wordpress"],
    "homepage"    : "https://taproot-theme.com",
    "license"     : "GPL-2.0-or-later",
    "authors"     : [
        {
            "name"    : "Sky Shabatura",
            "email"   : "theme@sky.camp",
            "homepage": "https://sky.camp"
        }
    ],
    "autoload"    : {
        "psr-4" : {
            "Taproot\\" : "app/"
        }
    },
    "repositories": [
        {
            "name": "justintadlock/hybrid-breadcrumbs",
            "type": "git",
            "url": "/Users/sky/Repos/justintadlock/hybrid-breadcrumbs"
        }
    ],
    "require": {
        "php" : ">=5.6",
        "composer/installers" : "~1.0",
        "justintadlock/hybrid-core" : "^5.1.0",
        "justintadlock/hybrid-breadcrumbs": "^1.0",
        "skyshab/rootstrap": "dev-master",
        "skyshab/rootstrap-devices": "dev-master",
        "skyshab/rootstrap-screens": "dev-master",
        "skyshab/rootstrap-styles": "dev-master",
        "skyshab/rootstrap-tabs": "^1.0",
        "skyshab/rootstrap-sequences": "dev-master",
        "skyshab/rootstrap-defaults": "dev-master",
        "skyshab/rootstrap-panels": "dev-master"
    },
	"require-dev": {
        "wptrt/wpthemereview": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "0.4.4"
	}
}
