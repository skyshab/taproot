{
    "name": "skyshabatura/taproot-2",
    "type": "wordpress-theme",
    "description": "Taproot is a multipurpose WordPress theme.",
    "keywords": [
        "wordpress"
    ],
    "homepage": "https://taproot-theme.com",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sky Shabatura",
            "email": "theme@sky.camp",
            "homepage": "https://sky.camp"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "Taproot\\": "app/"
        }
    },
    "repositories": [],
    "require": {
        "php": ">=5.6",
        "composer/installers": "~1.0",
        "justintadlock/hybrid-core": "dev-master",
        "justintadlock/hybrid-breadcrumbs": "dev-master",
        "skyshab/rootstrap": "dev-master",
        "skyshab/rootstrap-devices": "dev-master",
        "skyshab/rootstrap-screens": "dev-master",
        "skyshab/rootstrap-styles": "dev-master",
        "skyshab/rootstrap-tabs": "dev-master",
        "skyshab/rootstrap-sequences": "dev-master",
        "skyshab/rootstrap-defaults": "dev-master",
        "skyshab/rootstrap-panels": "dev-master"
    },
    "require-dev": {
        "wptrt/wpthemereview": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "0.4.4"
    }
}
